{
    "version": 2,
    "builds": [
        {
            "src": "./app.py",
            "use": "@vercel/python"
        }
    ],
    "build":{
        "env":{
            "PYTHONUNBUFFERED": "1", 
            "PYENCHANT_LIBRARY_PATH":"/usr/local/lib/libenchant.so"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ]
}